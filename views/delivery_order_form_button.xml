<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_inherit_delivery_order" model="ir.ui.view">
        <field name="name">stock.picking.form.delivery.order.button</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="%(rj_invoice.action_report_delivery_order_mini)d"
                        string="Custom Print"
                        type="action"
                        icon="fa-print"
                        class="oe_stat_button"/>
            </xpath>

        </field>
    </record>
</odoo>